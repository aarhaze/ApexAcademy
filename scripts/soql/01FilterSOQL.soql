
SELECT Id,
       FirstName,
       Phone
  FROM Contact
 WHERE Department IN ('Executive Team', 
                      'Finance',
                      'Technology')
   AND Phone != null
   AND Birthdate < 1995-06-15
   AND Title LIKE '%VP%' 
   
   
   
// WHERE must resolve to TRUE
// LIKE is similar to String.Contains 
// IN is multiple OR filters